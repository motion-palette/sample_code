   "source": [
    "s = \"a b c d e c\"\n",
    "\n",
    "# >> \"a b <tag>c</tag> d e c\"\n",
    "\n",
    "chars = s.split()\n",
    "#print(chars)\n",
    "lst = []\n",
    "for c in chars:\n",
    "    if c == 'c':\n",
    "        lst.append('\\n<tag>c</tag>\\n')\n",
    "    else:\n",
    "        lst.append(c)\n",
    "print(lst)        \n",
    "print(\"=====\")\n",
    "result = ' '.join(lst)\n",
    "print(result)"
   ]
